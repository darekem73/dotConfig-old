alias ll='ls -l'
alias la='ls -al'
alias l='ls'
alias li='ls | sxiv -i -t'
alias cls='printf "\033c"'
alias ms='monitor single'
alias me='monitor extend'
alias mc='monitor same'
alias ton='touchpadctl on'
alias toff='touchpadctl off'
alias rh='$(history | sort -rn | fzy | cut -b8-)'
alias d='date +"%Y-%m-%d_%H-%M-%S"'
